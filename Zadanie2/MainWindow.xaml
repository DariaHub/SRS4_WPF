<Window x:Class="Zadanie2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zadanie2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Корги 1">
                <Image Source="1.jpg" Stretch="Fill"/>
            </TabItem>
            <TabItem Header="Корги 2">
                <Image Source="2.jpg" Stretch="Fill"/>
            </TabItem>
            <TabItem Header="Корги 3">
                <Image Source="3.jpg" Stretch="Fill"/>
            </TabItem>
            <TabItem Header="Корги 4">
                <Image Source="4.jpg" Stretch="Fill"/>
            </TabItem>
            <TabItem Header="Корги 5" RenderTransformOrigin="0.5,0.509">
                <Image Source="5.jpg" Stretch="Fill"/>
            </TabItem>
            <TabItem Header="Корги 6">
                <Image Source="6.jpg" Stretch="Fill"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
